<configuration>
<system.web>
<customErrors mode="Off" />
<webServices>
        <protocols>
            <add name="HttpGet"/>
            <add name="HttpPost"/>
        </protocols>
    </webServices>
</system.web>
    <system.webServer>
        <security>
            <requestFiltering>
                <fileExtensions>
                    <remove fileExtension=".java" />
                </fileExtensions>
            </requestFiltering>
        </security>
    </system.webServer>
</configuration>
